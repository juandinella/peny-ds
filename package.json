{
  "name": "penny-ds",
  "version": "0.0.1",
  "description": "Penny Design System",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist & NODE_ENV=production BABEL_ENV=production rollup -c",
    "prepare": "npm run build",
    "test": "jest src --ci",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:coverage-web": "npm run test:coverage && open coverage/lcov-report/index.html",
    "lint": "eslint src/ --ext .jsx --ext .js",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cc": "node ./scripts/create-component",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juandinella/peny-ds.git"
  },
  "keywords": [],
  "author": "Juan Di Nella <juan.dinella@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juandinella/peny-ds/issues"
  },
  "files": [
    "dist/",
    "public/",
    "package.json",
    "README.md"
  ],
  "homepage": "https://github.com/juandinella/peny-ds#readme",
